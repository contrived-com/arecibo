# Arecibo runtime configuration
# Copy to .env in deployment environment.

# API authentication keys accepted via X-API-Key (comma-separated)
ARECIBO_API_KEYS=replace-with-secure-key

# Policy defaults
ARECIBO_POLICY_TTL_SEC=60
ARECIBO_POLICY_FILE=

# Deterministic GO_DARK verification controls
ARECIBO_FORCE_GO_DARK=false
ARECIBO_FORCE_GO_DARK_ON=
