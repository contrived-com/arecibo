{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "arecibo/schemas/ingest/events-batch/1.0.0",
  "type": "object",
  "required": [
    "schemaVersion",
    "batchId",
    "agentSessionId",
    "sentAt",
    "events"
  ],
  "properties": {
    "schemaVersion": {
      "const": "1.0.0"
    },
    "batchId": {
      "type": "string",
      "minLength": 1
    },
    "agentSessionId": {
      "type": "string",
      "minLength": 1
    },
    "sentAt": {
      "type": "string",
      "format": "date-time",
      "pattern": "Z$"
    },
    "events": {
      "type": "array",
      "minItems": 1,
      "maxItems": 1000,
      "items": {
        "$ref": "./app-event.1.0.0.json"
      }
    }
  },
  "additionalProperties": false
}
